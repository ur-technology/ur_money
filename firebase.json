{
  "hosting": {
    "public": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/typings/**"
    ],
    "rewrites": [ {
      "source": "**",
      "destination": "/index.html"
    } ],
    "headers": [ {
      "source" : "**/*",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=5"
      } ]
    } ]
  }
}
