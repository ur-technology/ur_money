{
  "firebase": "blinding-torch-3730",
  "public": "www",
  "ignore": [
    "firebase.json",
    "**/.*",
    "**/node_modules/**",
    "**/typings/**"
  ],
  "rewrites": [
    {"source": "/go/*", "destination": "/index.html"}
  ],
  "headers": [ {
    "source" : "**/*",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=5"
    } ]
  } ]
}
