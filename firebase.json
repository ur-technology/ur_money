{
  "hosting": {
    "public": "www",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/typings/**"
    ]
  },
  "rewrites": [
    {"source": "/go/*", "destination": "/go/index.html"},
    {"source": "/app", "destination": "/index.html"}
  ],
  "headers": [ {
    "source" : "**/*",
    "headers" : [ {
      "key" : "Cache-Control",
      "value" : "max-age=5"
    } ]
  } ]
}
